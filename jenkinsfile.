pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello dims'
                sleep 5
            }
        }
                stage('build') {
            steps {
                echo 'build dims'
                sleep 10
            }
        }
                stage('test') {
            steps {
                echo 'test dims'
                sleep5
                }
        }
                stage('deploy') {
                steps {
                echo 'deploy dims'
                sleep 5
            }
        }
    }
}
